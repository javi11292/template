{
  "name": "server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "docker": "docker-compose -f docker-compose.yml",
    "dev": "npm run docker -- -f docker-compose.development.yml up --build",
    "start": "npm run docker -- up --build"
  },
  "dependencies": {
    "cors": "^2.8.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.0"
  },
  "devDependencies": {
    "eslint": "^7.19.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-react-app": "^6.0.0",
    "nodemon": "^2.0.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "airbnb-base"
    ],
    "rules": {
      "consistent-return": "off",
      "import/no-extraneous-dependencies": "off",
      "import/prefer-default-export": "off",
      "no-use-before-define": "off",
      "no-console": "off"
    }
  }
}