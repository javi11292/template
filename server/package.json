{
  "name": "server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cd .. && docker-compose -f docker-compose.yml -f docker-compose.development.yml",
    "build": "npm run dev -- build",
    "start": "npm run dev -- up",
    "deploy": "cd .. && docker-compose up --build",
    "sh": "cd .. && docker-compose exec server sh"
  },
  "dependencies": {
    "compression": "^1.7.0",
    "express": "^4.17.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.0"
  }
}
