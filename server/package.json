{
  "name": "server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "docker": "cd .. && docker-compose -f docker-compose.yml",
    "development": "npm run docker -- -f docker-compose.development.yml",
    "production": "npm run docker -- -f docker-compose.production.yml",
    "build": "npm run development -- build",
    "dev": "npm run development -- up",
    "start": "npm run production -- up --build"
  },
  "dependencies": {
    "compression": "^1.7.0",
    "express": "^4.17.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "nodemon": "^2.0.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
