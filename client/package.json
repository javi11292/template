{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "start": "react-scripts start",
    "postbuild": "react-snap"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.0",
    "@material-ui/icons": "^4.9.0",
    "eztore": "^2.0.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-scripts": "^3.4.0"
  },
  "devDependencies": {
    "react-snap": "^1.23.0"
  },
  "reactSnap": {
    "puppeteerArgs": [
      "--no-sandbox"
    ],
    "puppeteerExecutablePath": "/usr/bin/chromium-browser",
    "asyncScriptTags": true,
    "inlineCss": true
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
