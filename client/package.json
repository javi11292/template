{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^10.0.0",
    "eztore": "^6.0.0",
    "material-components-web": "^10.0.0",
    "next": "^10.0.0",
    "next-offline": "^5.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "sass": "^1.32.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.13.10",
    "eslint": "^7.19.0",
    "eslint-config-airbnb": "^18.2.0"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "parser": "@babel/eslint-parser",
    "env": {
      "browser": true
    },
    "rules": {
      "consistent-return": "off",
      "no-use-before-define": "off",
      "import/prefer-default-export": "off",
      "react/jsx-filename-extension": "off",
      "react/jsx-props-no-spreading": "off",
      "react/prop-types": "off",
      "react/react-in-jsx-scope": "off"
    },
    "settings": {
      "import/resolver": {
        "node": {
          "paths": "src"
        }
      }
    }
  },
  "babel": {
    "presets": [
      "next/babel"
    ]
  }
}