{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^10.0.0",
    "eztore": "^6.0.0",
    "material-components-web": "^10.0.0",
    "next": "^10.0.0",
    "next-offline": "^5.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "sass": "^1.32.0"
  },
  "devDependencies": {
    "eslint": "^7.19.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-react-app": "^6.0.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "airbnb-base"
    ],
    "rules": {
      "consistent-return": "off",
      "import/no-extraneous-dependencies": "off",
      "import/prefer-default-export": "off",
      "no-use-before-define": "off"
    },
    "settings": {
      "import/resolver": {
        "node": {
          "paths": "src"
        }
      }
    }
  },
  "babel": {
    "presets": [
      "next/babel"
    ]
  }
